st.termname:     st-256color

! Set to a non-zero value to disable window decorations (titlebar, etc) and go borderless.
st.borderless:        0

! --- The following options options can be reloaded via USR1 signal. ---
st.font:              JetBrainsMono Nerd Font Mono:pixelsize=23:antialias=true:autohint=true;

! window padding
st.borderpx:          0

! Kerning / character bounding-box height multiplier
st.chscale:           1.0

! Kerning / character bounding-box width multiplier
st.cwscale:           1.0

!! special
!*.foreground:   #ffffff
!*.background:   #000000
!*.cursorColor:  #ffffff
!
! black
*.color0:       #252525
*.color8:       #454545

! red
*.color1:       #ff9f95
*.color9:       #ff8d80

! green
*.color2:       #a6e22e
*.color10:      #b6e354

! yellow
*.color3:       #fd971f
*.color11:      #fd971f

! blue
*.color4:       #435e87
*.color12:      #587aa4

! magenta
*.color5:       #789ec6
*.color13:      #46a4ff

! cyan
*.color6:       #5e7175
*.color14:      #a3babf

! white
*.color7:       #dbdcdc
*.color15:      #fdfdfd
